<div class="container">	<div>		<form id="form1" v-on:submit.prevent="">			<div class="form-group row">				<label for="title" class="col-sm-2 col-form-label">露營區名稱</label>				<div class="col-sm-8">					<input type="text" class="form-control" id="name" v-model="myCampingAreas.name" required>				</div>			</div>			<div class="form-group row">				<label for="price" class="col-sm-2 col-form-label">電話</label>				<div class="col-sm-8">					<input type="tel" placeholder="0123-445-678" pattern="[0-9]{4}-[0-9]{3}-[0-9]{3}" required class="form-control" id="phone" v-model="myCampingAreas.phone">				</div>			</div>			<div class="form-group row">				<label for="myCampingAreas.region" class="col-sm-2 col-form-label">地區</label>				<select v-model="myCampingAreas.region">					<option v-for="region in regions" v-bind:value="region.value">						{{ region.text }}					</option>				</select>				<span>Selected: {{ myCampingAreas.region }}</span>			</div>			<div class="form-group row">				<label for="description" class="col-sm-2 col-form-label">地址</label>				<div class="col-sm-8">					<input type="text" class="form-control" id="address" v-model="myCampingAreas.address">				</div>			</div>			<div class="form-group row">				<label for="description" class="col-sm-2 col-form-label">敘述</label>				<div class="col-sm-8">					<textarea class="form-control" id="description" v-model="myCampingAreas.description"></textarea>				</div>			</div>			<div id='example'>				服務設施				<div v-for="(value, key, index) in myServices" :key="index">					<input type="checkbox" :id="key" v-model="myServices[key]">					<label :for="key">{{ key }} {{ value }}</label>				</div><button v-on:click="aaa">aaa</button>			</div>			<div class="form-group row">				<label for="pic" class="col-sm-2 col-form-label">露營區照片</label>				<div class="col-sm-8">					<input type="file" class="form-control" id="pic" v-on:change="up" multiple="multiple">				</div>			</div>			<div class="border p-2 mt-3">				<p>Preview Here:</p>				<template v-if="picture.preview_list.length">					<div v-for="(item, index) in picture.preview_list" :key="index">						<img :src=" item" class="img-fluid" />						<button class="btn-danger" v-on:click="deletepreviewpic(index)">刪除</button>					</div>				</template>				<template v-if="myCampingAreas.path.length != 0">					<div v-for="(item, index) in myCampingAreas.path" :key="index">						<img :src="'/Camping_Area_Picture/'+item" class="img-fluid" />						<button class="btn-danger" v-on:click="deletepic(index)">刪除</button>					</div>				</template>			</div>			<div class="form-group row">				<label for="pic" class="col-sm-2 col-form-label">操作</label>				<div class="col-sm-8">					<button v-on:click="send">新增</button>				</div>			</div>		</form>	</div></div><script>	var app = new Vue(		{			el: "#form1",			data: {				myCampingAreas: {},				myServices: {},				picture: {					preview: null,					preview_list: [],					image_list: []				},				regions: [					{ text: '基隆', value: 'Keelung' },					{ text: '新北', value: 'New_Taipei' },					{ text: '台北', value: 'Taipei' },					{ text: '桃園', value: 'Taoyuan' },					{ text: '新竹', value: 'Hsinchu' },					{ text: '苗栗', value: 'Miaoli' },					{ text: '台中', value: 'Taichung' },					{ text: '彰化', value: 'Changhua' },					{ text: '南投', value: 'Nantou' },					{ text: '雲林', value: 'Yunlin' },					{ text: '嘉義', value: 'Chiayi' },					{ text: '台南', value: 'Tainan' },					{ text: '高雄', value: 'Kaohsiung' },					{ text: '屏東', value: 'Pingtung' },					{ text: '宜蘭', value: 'Yilan' },					{ text: '花蓮', value: 'Hualien' },					{ text: '台東', value: 'Taitung' }				]			},			methods: {				up: function (a) {					//console.log(a);					//console.log(a.target.files[0]);					var input = a.target;					var count = input.files.length;					var index = 0;										if (input.files) {						while (count--) {							var reader = new FileReader();							reader.onload = (e) => {								this.picture.preview_list.push(e.target.result);							}							this.picture.image_list.push(input.files[index]);							reader.readAsDataURL(input.files[index]);							index++;							alert("上傳了!!!");							console.log(this.picture.image_list);							console.log(this.picture.image_list);						}					}															for (name in this.services) {						var a = document.querySelector(`#${name}`);						console.log(a.checked);					}				},				aaa: function () {					for (name in this.names) {						var a = document.querySelector(`#${name}`);						console.log(a.checked);					}				},				deletepic: function (a)				{					var self = this;					axios.post("/Edit_Camping_Area/DeleteCampingPicture/" + self.myCampingAreas.pathId[a]).then(function (res) {
						if (res.data == true) {
							self.myCampingAreas.path.splice(a, 1);
							console.log("刪除了")
							alert("刪成功!!!")
							//window.location.reload();
						}
						else {
							alert("刪失敗!!!")
						}
					});
				},				deletepreviewpic: function (a)				{					this.picture.preview_list.splice(a, 1);					this.picture.image_list.splice(a, 1);					//window.location.reload();				},				send: function () {					var bodyFormData = new FormData();					bodyFormData.append("Name", this.myCampingAreas.name);					bodyFormData.append("Phone", this.myCampingAreas.phone);					bodyFormData.append("Region", this.myCampingAreas.region);					bodyFormData.append("Address", this.myCampingAreas.address);					bodyFormData.append("Description", this.myCampingAreas.description);					for (let service in this.myServices) {						var a = document.querySelector(`#${service}`);						bodyFormData.append(`${service}`, a.checked)					}					for (item in this.myCampingAreas)					{											//	var de = JSON.stringify({ item:this.myCampingAreas[item] }); console.log(de);						bodyFormData.append(`MyCampingAreas[${item}]`, this.myCampingAreas[item]);					}										//bodyFormData.append("MyServices", this.myServices);					for (let index = 0; index < this.picture.image_list.length; index++) {						bodyFormData.append("Path", this.picture.image_list[index]);					}					axios({						method: "post",						url: "/Edit_Camping_Area/EditCamping_Area_Into_DB",						data: bodyFormData,						headers: { "Content-Type": "multipart/form-data" },					})						.then(function (response) {							if (response.data == true) {								alert("success");								window.location.href = "/Camping_Information/CampingInformation";							}							else {								alert("失敗");							}							//	//handle success						})						.catch(function (response) {							//handle error							console.log(response);						});				}			},			mounted: function () {
				var self = this;
				axios.get("/Edit_Camping_Area/GetCamping_Area").then(function (res) {
					self.myCampingAreas = res.data.myCampingAreas;
					self.myServices = res.data.myServices
					console.log(res.data);
					console.log(self.myCampingAreas);
					console.log(self.myCampingAreas.path)
					//console.log(self.myServices);
					//console.log(res.data.myService[0]);
					//self.picture.preview_list = self.myCampingAreas.path;
					for (var item of self.picture.preview_list) {
						 console.log(item)
						
					}
						
				});
			}		}	);</script>